		"class-validator": "^0.14.0",
		"dynamoose": "^3.2.1",
		"jest-mock": "^29.7.0",
		"kafkajs": "^2.2.4",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"trino-client": "^0.2.1",
		"uuid": "^9.0.0",
		"uuidv4": "^6.2.13"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.2.1",
		"@nestjs/schematics": "^10.0.3",
		"@nestjs/testing": "^10.2.10",
		"@types/express": "^4.17.21",
		"@types/jest": "27.5.0",
		"@types/multer": "^1.4.10",
		"@types/node": "^16.0.0",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "28.0.3",
		"prettier": "^2.3.2",
		"prisma": "5.6.0",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "28.0.1",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.0.0",
		"typescript": "^5.3.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
